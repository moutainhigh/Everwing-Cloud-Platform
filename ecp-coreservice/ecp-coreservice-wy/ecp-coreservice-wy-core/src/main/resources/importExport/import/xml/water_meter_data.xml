            <field index="3" property="totalReading"  required="true" type="Double" regex="^(([1-9][0-9]*)|((([1-9][0-9]*)|0)\.[0-9]{1,2}))$" regexErrMsg="该项只能输入是数字"></field>
            <field index="4" property="remark"></field>
        </list>
    </sheet>
</excel>