            <field index="13" property="rate" type="Float" required="true" regex="^(([1-9][0-9]*)|((([1-9][0-9]*)|0)\.[0-9]{1,2}))$" regexErrMsg="该项只能输入是数字"></field>
            <field index="14" property="projectId" required="true"></field>
            <field index="15" property="relationBuilding"></field>
            <field index="16" property="assemblePerson"></field>
            <field index="17" property="brand"></field>
            <field index="18" property="specs"></field>
            <field index="19" property="provider"></field>
            <field index="20" property="providerPhone"></field>
            <field index="21" property="parentCode"></field>
            <field index="22" property="masterCode"></field>
            <field index="23" property="meterLevel" required="true" type="Map" format="1:一级水表;2:二级水表;3:三级水表;4:四级水表;5:五级水表;6:六级水表;7:七级水表;8:八级水表"></field>
