            <field index="6" property="userType" required="true" type="Map" format="0:商用;1:民用;2:管理处"></field>
            <field index="7" property="isBilling" required="true" type="Map" format="0:是;1:否"></field>
            <field index="8" property="isPublic" required="true" type="Map" format="0:是;1:否"></field>
            <field index="9" property="minAmount" type="Double" required="true" regex="^(([1-9][0-9]*)|((([1-9][0-9]*)|0)\.[0-9]{1,2}))$" regexErrMsg="该项只能输入是数字"></field>
            <field index="10" property="maxAmount" type="Double" required="true" regex="^(([1-9][0-9]*)|((([1-9][0-9]*)|0)\.[0-9]{1,2}))$" regexErrMsg="该项只能输入是数字"></field>
            <field index="11" property="initAmount" type="Double" required="true" regex="^(([1-9][0-9]*)|((([1-9][0-9]*)|0)\.[0-9]{1,2}))$" regexErrMsg="该项只能输入是数字"></field>
