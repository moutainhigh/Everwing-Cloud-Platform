<?xml version="1.0" encoding="UTF-8"?>
<excel id="water_meter">
    <sheet index="0">
        <list index="0" startRow="1" class="com.everwing.coreservice.common.wy.entity.business.watermeter.TcWaterMeter">
            <field index="0" property="code" required="true"></field>
            <field index="1" property="waterMeterName" required="true"></field>
