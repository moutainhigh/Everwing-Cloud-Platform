<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" ><mapper namespace="com.everwing.coreservice.wy.dao.mapper.materlog.WaterlogMapper">	<resultMap id="BaseResultMap" type="TcWaterlog">		<result property="id" column="id"/>		<result property="waterId" column="water_id"/>		<result property="operator" column="operator"/>		<result property="modifyTime" column="modifyTime"/>		<result property="modifyType" column="modifyType"/>		<result property="modifyMatter" column="modifyMatter"/>		<result property="code" column="code"/>		<association property="tcWaterMeter" column="water_id"					 javaType="TcWaterMeter"					  select="com.everwing.coreservice.wy.dao.mapper.property.TcSurfaceWaterMapper.SeleteByID" />	</resultMap>	<insert id="insert" parameterType="TcWaterlog" >		INSERT INTO   tc_waterlog (id,code,water_id,operator,modifyTime,modifyType,modifyMatter)		VALUES(		UUID(),		#{code},		#{waterId},		#{operator},		now(),		#{modifyType},		#{modifyMatter}		)	</insert>	<select id="listPage" resultMap="BaseResultMap">		select * from tc_waterlog where 1 = 1		<if test="waterId != '' and waterId != null">			and water_id = #{waterId}		</if>		<if test="operator != '' and operator != null">			and operator = #{operator}		</if>		<if test="modifyTime != '' and modifyTime != null">			and modifyTime = #{modifyTime}		</if>		<if test="modifyType != '' and modifyType != null">			and modifyType = #{modifyType}		</if>		<if test="modifyMatter != '' and modifyMatter != null">			and modifyMatter = #{modifyMatter}		</if>	</select></mapper>