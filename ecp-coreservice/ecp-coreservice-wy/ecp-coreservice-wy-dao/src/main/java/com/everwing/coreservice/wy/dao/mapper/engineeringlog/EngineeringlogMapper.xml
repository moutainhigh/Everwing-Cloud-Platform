<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" ><mapper namespace="com.everwing.coreservice.wy.dao.mapper.engineeringlog.EngineeringlogMapper">	<resultMap id="BaseResultMap" type="Engineeringlog">		<result property="id" column="id"/>		<result property="buildingid" column="building_id"/>		<result property="operator" column="operator"/>		<result property="modifyTime" column="modifyTime"/>		<result property="modifyType" column="modifyType"/>		<result property="modifyMatter" column="modifyMatter"/>		<result property="houseCodeNew" column="house_code_new"/>		<result property="engineeringName" column="engineering_name"/>		<result property="constructionAddr" column="construction_addr"/>		<result property="engineeringUnit" column="engineering_unit"/>		<result property="engineeringDirector" column="engineering_director"/>		<association property="tcConstruction" column="building_id"					 javaType="TcConstruction"					  select="com.everwing.coreservice.wy.dao.mapper.property.TcConstructionMapper.SeleteByID" />	</resultMap>	<insert id="insert" parameterType="Engineeringlog" >		INSERT INTO tc_engineeringlog (id,		building_id,		operator,		modifyTime,		modifyType,		modifyMatter,		house_code_new,engineering_name,construction_addr,engineering_unit,engineering_director		)		VALUES(		UUID(),		#{buildingid},		#{operator},		now(),		#{modifyType},		#{modifyMatter},		#{houseCodeNew},#{engineeringName},#{constructionAddr},#{engineeringUnit},#{engineeringUnit}		)	</insert>	<select id="listPage" resultMap="BaseResultMap">		select * from tc_engineeringlog where 1 = 1		<if test="buildingid != '' and buildingid != null">			and building_id = #{buildingid}		</if>		<if test="operator != '' and operator != null">			and operator = #{operator}		</if>		<if test="modifyTime != '' and modifyTime != null">			and modifyTime = #{modifyTime}		</if>		<if test="modifyType != '' and modifyType != null">			and modifyType = #{modifyType}		</if>		<if test="modifyMatter != '' and modifyMatter != null">			and modifyMatter = #{modifyMatter}		</if>	</select></mapper>