<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" ><mapper namespace="com.everwing.coreservice.wy.dao.mapper.operation.OperationMapper">	<resultMap id="BaseResultMap" type="Operationlog">		<result property="id" column="id"/>		<result property="address" column="address"/>		<result property="houseCodeNew" column="house_code_new"/>		<result property="buildingFullName" column="building_full_name"/>		<result property="address" column="address"/>		<result property="operator" column="operator"/>		<result property="modifyTime" column="modifyTime"/>		<result property="modifyType" column="modifyType"/>		<result property="modifyMatter" column="modifyMatter"/>	</resultMap>	<insert id="insert" parameterType="Operationlog" >		INSERT INTO tc_operationlog (id,building_id,building_full_name,house_code_new,address,operator,modifyTime,modifyType,modifyMatter)		VALUES(		UUID(),		#{buildingid},		#{buildingFullName},		#{houseCodeNew},		#{address},		#{operator},		now(),		#{modifyType},		#{modifyMatter}		)	</insert>	<select id="listPage" resultMap="BaseResultMap">		select * from tc_operationlog where 1 = 1		<if test="buildingid != '' and buildingid != null">			and building_id = #{buildingid}		</if>		<if test="houseCodeNew != '' and houseCodeNew != null">			and house_code_new = #{houseCodeNew}		</if>		<if test="buildingFullName != '' and buildingFullName != null">			and building_full_name = #{buildingFullName}		</if>		<if test="address != '' and address != null">			and address = #{address}		</if>		<if test="operator != '' and operator != null">			and operator = #{operator}		</if>		<if test="modifyTime != '' and modifyTime != null">			and modifyTime = #{modifyTime}		</if>		<if test="modifyType != '' and modifyType != null">			and modifyType = #{modifyType}		</if>		<if test="modifyMatter != '' and modifyMatter != null">			and modifyMatter = #{modifyMatter}		</if>	</select></mapper>