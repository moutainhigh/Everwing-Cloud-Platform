<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" ><mapper namespace="com.everwing.coreservice.wy.dao.mapper.materlog.ElectlogMapper">	<resultMap id="BaseResultMap" type="TcElectlog">		<result property="id" column="id"/>		<result property="electId" column="elect_id"/>		<result property="operator" column="operator"/>		<result property="modifyTime" column="modifyTime"/>		<result property="modifyType" column="modifyType"/>		<result property="modifyMatter" column="modifyMatter"/>		<result property="code" column="code"/>		<association property="tcElectMeter" column="elect_id"					 javaType="TcElectMeter"					  select="com.everwing.coreservice.wy.dao.mapper.property.TcSurfaceElectMapper.SeleteByID" />	</resultMap>	<insert id="insert" parameterType="TcElectlog" >		INSERT INTO tc_electlog (id,code,elect_id,operator,modifyTime,modifyType,modifyMatter)		VALUES(		UUID(),		#{code},		#{electId},		#{operator},		now(),		#{modifyType},		#{modifyMatter}		)	</insert>	<select id="listPage" resultMap="BaseResultMap">		select * from tc_electlog where 1 = 1		<if test="electId != '' and electId != null">			and elect_id = #{electId}		</if>		<if test="operator != '' and operator != null">			and operator = #{operator}		</if>		<if test="modifyTime != '' and modifyTime != null">			and modifyTime = #{modifyTime}		</if>		<if test="modifyType != '' and modifyType != null">			and modifyType = #{modifyType}		</if>		<if test="modifyMatter != '' and modifyMatter != null">			and modifyMatter = #{modifyMatter}		</if>	</select></mapper>