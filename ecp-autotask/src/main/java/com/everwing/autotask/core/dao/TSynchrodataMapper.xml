<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" ><mapper namespace="com.everwing.autotask.core.dao.TSynchrodataMapper">	<resultMap id="ResultMap" type="TSynchrodata">		<result property="id" column="id"/>		<result property="code" column="code"/>		<result property="tableName" column="table_name"/>		<result property="tableFieldName" column="table_field_name"/>		<result property="tableFieldValue" column="table_field_value"/>		<result property="destinationQueue" column="destination_queue"/>		<result property="operation" column="operation"/>		<result property="priorityLevel" column="priority_level"/>		<result property="state" column="state"/>		<result property="description" column="description"/>		<result property="errorMessage" column="error_message"/>		<result property="synchroTime" column="synchro_time"/>		<result property="createrId" column="creater_id"/>		<result property="createrName" column="creater_name"/>		<result property="createTime" column="create_time"/>		<result property="modifyId" column="modify_id"/>		<result property="modifyName" column="modify_name"/>		<result property="modifyTime" column="modify_time"/>	</resultMap>	<resultMap id="ListResultMap" type="TSynchrodataList" extends="ResultMap">	</resultMap>	<select id="findByCondition" parameterType="TSynchrodataSearch" resultMap="ListResultMap">		SELECT * FROM t_synchrodata t WHERE 1=1		<if test="id != null and id !=  ''">			AND t.id = #{id}		</if>		<if test="code != null and code !=  ''">			AND t.code = #{code}		</if>		<if test="tableName != null and tableName !=  ''">			AND t.table_name = #{tableName}		</if>		<if test="tableFieldName != null and tableFieldName !=  ''">			AND t.table_field_name = #{tableFieldName}		</if>		<if test="tableFieldValue != null and tableFieldValue !=  ''">			AND t.table_field_value = #{tableFieldValue}		</if>		<if test="state != null and state !=  ''">			AND t.state = #{state}		</if>		<if test="priorityLevel != null and priorityLevel !=  ''">			AND t.priority_level = #{priorityLevel}		</if>	</select>	<select id="findDestinationTableDataBySQL" parameterType="java.util.Map" resultType="java.util.Map">		${sql}	</select>	<insert id="batchInsert">		INSERT INTO t_synchrodata		(			id,			code,			table_name,			table_field_name,			table_field_value,			destination_queue,			operation,			priority_level,			state,			description,			error_message,			synchro_time,			creater_id,			creater_name,			create_time		)		VALUES		<foreach collection="list" index="index" item="item" separator=",">		(			UUID(),		 	#{item.code},			#{item.tableName},			#{item.tableFieldName},			#{item.tableFieldValue},			#{item.destinationQueue},			#{item.operation},			#{item.priorityLevel},			#{item.state},			#{item.description},			#{item.errorMessage},			#{item.synchroTime},			#{item.createrId},			#{item.createrName},			sysdate()		)		</foreach>	</insert>	<update id="batchModify" parameterType="java.util.List">		<foreach collection="list" separator=";" index="index" item="item">			UPDATE t_synchrodata SET				state = #{item.state},				synchro_time =  sysdate(),				modify_id = #{item.modifyId},				modify_name = #{item.modifyName},				modify_time = sysdate()			WHERE id = #{item.id}		</foreach>	</update></mapper>