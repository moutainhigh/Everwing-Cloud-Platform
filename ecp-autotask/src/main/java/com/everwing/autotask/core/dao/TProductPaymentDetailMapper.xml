<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" ><mapper namespace="com.everwing.autotask.core.dao.TProductPaymentDetailMapper">	<resultMap id="BaseResultMap" type="TProductPaymentDetail">		<result property="id" column="id"/>		<result property="orderId" column="order_id"/>		<result property="orderBatchNo" column="order_batch_no"/>		<result property="businessSerialNumber" column="business_serial_number"/>		<result property="payType" column="pay_type"/>		<result property="price" column="price"/>		<result property="createrId" column="creater_id"/>		<result property="createrName" column="creater_name"/>		<result property="createTime" column="create_time"/>	</resultMap>	<resultMap id="ListResultMap" type="TProductPaymentDetailList" extends="BaseResultMap">		<result property="payTypeName" column="pay_type_name"/>	</resultMap>	<select id="findByCondition" parameterType="TProductPaymentDetailSearch" resultMap="ListResultMap">		select			t.*,			(SELECT li.`name` FROM t_sys_lookup_item li WHERE li.parent_code = 'payType' AND li.`code` = t.pay_type) as pay_type_name		from t_product_payment_detail t where 1=1		<if test="id != null and id != ''">			AND t.id = #{id}		</if>		<if test="orderId != null and orderId != ''">			AND t.order_id = #{orderId}		</if>		<if test="orderBatchNo != null and orderBatchNo != ''">			AND t.order_batch_no = #{orderBatchNo}		</if>		<if test="payType != null and payType != ''">			AND t.pay_type = #{payType}		</if>		ORDER BY t.create_time DESC	</select>	<delete id="delete" parameterType="String">		DELETE FROM t_product_payment_detail  where order_batch_no = #{0}	</delete>	<insert id="batchInsert" parameterType="List">		INSERT INTO t_product_payment_detail		(			id,			order_id,			order_batch_no,			business_serial_number,			pay_type,			price,			creater_id,			creater_name,			create_time		)		VALUES		<foreach collection="list" index="index" item="item" separator=",">		(			UUID(),			#{item.orderId},			#{item.orderBatchNo},			#{item.businessSerialNumber},			#{item.payType},			#{item.price},			#{item.createrId},			#{item.createrName},			sysdate()		)		</foreach>	</insert></mapper>